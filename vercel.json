{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/public/css/(.*)", "dest": "css/$1" },
    { "src": "/public/img/(.*)", "dest": "/img/$1" },
    { "src": "/public/js/(.*)", "dest": "/js/$1" },
    { "src": "/(.*)", "dest": "index.js" }
  ]
}
